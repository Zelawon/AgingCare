<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FonctionsSurveillant.Traitement.AjouterTraitement">

    <Button
        android:id="@+id/ajouterTraitementResident"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:text="Ajouter"
        app:layout_constraintBottom_toBottomOf="@+id/retourFrAjoutTraitement"
        app:layout_constraintStart_toEndOf="@+id/retourFrAjoutTraitement"
        app:layout_constraintTop_toTopOf="@+id/retourFrAjoutTraitement" />

    <Button
        android:id="@+id/retourFrAjoutTraitement"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="10dp"
        android:text="Retour"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dateDebutTraitement"
        app:layout_constraintVertical_bias="0.50" />

    <TextView
        android:id="@+id/textView92"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Ajouter Traitement"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView93"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:text="Type traitement:"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView92" />

    <Spinner
        android:id="@+id/spinnerTypeTraitement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/textView93"
        app:layout_constraintTop_toTopOf="@+id/textView93" />

    <Spinner
        android:id="@+id/spinnerRepetitionTraitement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/textView99"
        app:layout_constraintTop_toTopOf="@+id/textView99" />

    <TextView
        android:id="@+id/textView102"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Nombre répétition par jour:"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textViewRepetitionTrait"
        app:layout_constraintTop_toBottomOf="@+id/weekdays_sample_2" />

    <Spinner
        android:id="@+id/spinnerRepetitionTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView102"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/textView102"
        app:layout_constraintTop_toTopOf="@+id/textView102" />

    <TextView
        android:id="@+id/textView94"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="Nom traitement:"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView93" />

    <EditText
        android:id="@+id/nomTraitement"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/textView94"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView94"
        app:layout_constraintTop_toTopOf="@+id/textView94"
        app:layout_constraintVertical_bias="0.60" />

    <TextView
        android:id="@+id/textView96"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Dose:"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView94"
        app:layout_constraintTop_toBottomOf="@+id/nomTraitement" />

    <TextView
        android:id="@+id/textView99"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Répétition:"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView96"
        app:layout_constraintTop_toBottomOf="@+id/doseTraitement" />

    <TextView
        android:id="@+id/uniteTraitement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="unite"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/doseTraitement"
        app:layout_constraintStart_toEndOf="@+id/doseTraitement"
        app:layout_constraintTop_toTopOf="@+id/doseTraitement" />

    <EditText
        android:id="@+id/doseTraitement"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toBottomOf="@+id/textView96"
        app:layout_constraintStart_toEndOf="@+id/textView96"
        app:layout_constraintTop_toTopOf="@+id/textView96"
        app:layout_constraintVertical_bias="0.60" />


    <TextView
        android:id="@+id/textViewRepetitionTrait"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Choisir jours répétition:"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView99"
        app:layout_constraintTop_toBottomOf="@+id/textView99" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/weekdays_sample_2"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewRepetitionTrait" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView102">

        <TextView
            android:id="@+id/textTempTaitement1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginStart="10dp"
            android:text="Temp 1:"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textTempTaitement3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginTop="10dp"
            android:text="Temp 3:"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/textTempTaitement1"
            app:layout_constraintTop_toBottomOf="@+id/textTempTaitement1" />

        <TextView
            android:id="@+id/textTempTaitement5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginTop="10dp"
            android:text="Temp 5:"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/textTempTaitement3"
            app:layout_constraintTop_toBottomOf="@+id/textTempTaitement3" />

        <TextView
            android:id="@+id/tempTaitement1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginStart="5dp"
            android:text="HH:MM"
            android:textAllCaps="false"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/textTempTaitement1"
            app:layout_constraintStart_toEndOf="@+id/textTempTaitement1"
            app:layout_constraintTop_toTopOf="@+id/textTempTaitement1" />

        <TextView
            android:id="@+id/tempTaitement2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginEnd="10dp"
            android:text="HH:MM"
            android:textAllCaps="false"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/textTempTaitement2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textTempTaitement2" />

        <TextView
            android:id="@+id/tempTaitement3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginStart="5dp"
            android:text="HH:MM"
            android:textAllCaps="false"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/textTempTaitement3"
            app:layout_constraintStart_toEndOf="@+id/textTempTaitement3"
            app:layout_constraintTop_toTopOf="@+id/textTempTaitement3" />

        <TextView
            android:id="@+id/tempTaitement5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginStart="5dp"
            android:text="HH:MM"
            android:textAllCaps="false"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/textTempTaitement5"
            app:layout_constraintStart_toEndOf="@+id/textTempTaitement5"
            app:layout_constraintTop_toTopOf="@+id/textTempTaitement5"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tempTaitement6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginStart="5dp"
            android:text="HH:MM"
            android:textAllCaps="false"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/textTempTaitement6"
            app:layout_constraintStart_toEndOf="@+id/textTempTaitement6"
            app:layout_constraintTop_toTopOf="@+id/textTempTaitement6"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textTempTaitement6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginTop="10dp"
            android:text="Temp 6:"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/textTempTaitement4"
            app:layout_constraintTop_toBottomOf="@+id/textTempTaitement4" />

        <TextView
            android:id="@+id/tempTaitement4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginStart="5dp"
            android:text="HH:MM"
            android:textAllCaps="false"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/textTempTaitement4"
            app:layout_constraintStart_toEndOf="@+id/textTempTaitement2"
            app:layout_constraintTop_toTopOf="@+id/textTempTaitement4"
            app:layout_constraintVertical_bias="0.50" />

        <TextView
            android:id="@+id/textTempTaitement4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginTop="10dp"
            android:text="Temp 4:"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/textTempTaitement2"
            app:layout_constraintTop_toBottomOf="@+id/textTempTaitement2" />

        <TextView
            android:id="@+id/textTempTaitement2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/poidsRes"
            android:layout_marginEnd="5dp"
            android:text="Temp 2:"
            android:textSize="18sp"
            app:layout_constraintEnd_toStartOf="@+id/tempTaitement2"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/dateDebutTraitement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/poidsRes"
        android:text="AAAA/MM/JJ"
        android:textAllCaps="false"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/textViewDatePoids13"
        app:layout_constraintTop_toBottomOf="@+id/textViewDatePoids13" />

    <TextView
        android:id="@+id/dateFinTraitement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/poidsRes"
        android:text="AAAA/MM/JJ"
        android:textAllCaps="false"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/textViewDatePoids14"
        app:layout_constraintTop_toBottomOf="@+id/textViewDatePoids14" />

    <TextView
        android:id="@+id/textViewDatePoids14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/poidsRes"
        android:layout_marginStart="100dp"
        android:layout_marginTop="20dp"
        android:text="Date fin:"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/textViewDatePoids13"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout5" />

    <TextView
        android:id="@+id/textViewDatePoids13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/poidsRes"
        android:layout_marginTop="20dp"
        android:text="Date debut:"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView102"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout5" />


</androidx.constraintlayout.widget.ConstraintLayout>